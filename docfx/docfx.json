{
  "metadata": [
    {
      "src": [
        {
          "files": [
              "Metalama/Metalama.Framework/Metalama.Framework.csproj",
              "Metalama/Metalama.TestFramework/Metalama.TestFramework.csproj",
              "Metalama/Metalama.Framework.Sdk/Metalama.Framework.Sdk.csproj",
              "Metalama/Metalama.Framework.Introspection/Metalama.Framework.Introspection.csproj",
              "Metalama/Metalama.Framework.Workspaces/Metalama.Framework.Workspaces.csproj",
              "Metalama/Metalama.LinqPad/Metalama.LinqPad.csproj",
              "Metalama.Compiler/src/Metalama/Metalama.Compiler.Sdk/Metalama.Compiler.Sdk.csproj"
              ],
          "src": "c:/src"
        }
      ],
      "dest": "obj/api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "properties": {
        "TargetFramework": "net5.0"}
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ],
        "src": "obj"
      },
      {
        "files": [
          "**.md",
          "toc.yml"
        ],
        "src": "../conceptual"
      }
    ],
    "resource": [
      {
        "files": [
          "**.png"
        ],
        "src": "../conceptual"
      }
    ],
    "overwrite": [
      {
        "files": [
          "namespaces/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "xrefService": [
      "https://xref.docs.microsoft.com/query?uid={uid}"
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template":[ 
      "default", 
      "packages/memberpage.2.59.0/content",
      "../code/Metalama.Documentation.DfmExtensions/bin/Debug"],
    "postProcessors": [],
    "markdownEngineName": "dfm",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": true
  }
}
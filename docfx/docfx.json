{
  "metadata": [
    {
      "src": [
        {
          "files": [
              "Metalama.Framework.dll",
              "Metalama.TestFramework.dll",
              "Metalama.Framework.Sdk.dll",
              "Metalama.Framework.Introspection.dll",
              "Metalama.Framework.Workspaces.dll",
              "Metalama.LinqPad.dll",
              "Metalama.Compiler.Interface.dll"
              ],
          "src": "../code/Metalama.Documentation.Consolidated/bin/Debug/net6.0-windows"
        }
      ],
      "dest": "obj/api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ],
        "src": "obj"
      },
      {
        "files": [
          "**.md",
          "toc.yml"
        ],
        "src": "../conceptual"
      }
    ],
    "resource": [
      {
        "files": [
          "**.png"
        ],
        "src": "../conceptual"
      }
    ],
    "overwrite": [
      {
        "files": [
          "namespaces/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "xrefService": [
      "https://xref.docs.microsoft.com/query?uid={uid}"
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template":[ 
      "default", 
      "packages/memberpage.2.59.0/content",
      "../code/Metalama.Documentation.DfmExtensions/bin/Debug"],
    "postProcessors": [],
    "markdownEngineName": "dfm",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false
  }
}
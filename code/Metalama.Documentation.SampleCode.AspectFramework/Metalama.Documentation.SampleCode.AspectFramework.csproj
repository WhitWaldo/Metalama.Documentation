<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <OutputType>Library</OutputType>
        <TargetFramework>net6.0</TargetFramework>
        <StartupObject>AutoGeneratedProgram</StartupObject>
        <RootNamespace>Doc</RootNamespace>
        <LangVersion>9.0</LangVersion>
    </PropertyGroup>

    <ItemGroup>
      <Compile Remove="InheritedTypeLevel - Copy.t.cs" />
    </ItemGroup>

    <ItemGroup>
      <None Include="InheritedMethodLevel.t.cs">
        <SubType>Code</SubType>
        <DependentUpon>InheritedMethodLevel.cs</DependentUpon>
      </None>
    </ItemGroup>

    <ItemGroup>
        <PackageReference Include="Microsoft.NET.Test.Sdk" Version="16.7.1" />
        <PackageReference Include="Microsoft.Win32.Registry" Version="5.0.0" />
        <PackageReference Include="xunit" Version="2.4.1" />
        <PackageReference Include="xunit.runner.visualstudio" Version="2.4.3">
            <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
            <PrivateAssets>all</PrivateAssets>
        </PackageReference>

        <PackageReference Include="Metalama.Framework" Version="$(MetalamaVersion)" />
        <PackageReference Include="Metalama.TestFramework" Version="$(MetalamaVersion)" />
    </ItemGroup>

</Project>

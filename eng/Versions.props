<Project>

    <!-- Set the default version -->
    <Import Project="MainVersion.props" />
    
    <PropertyGroup>
        <CaravelaSamplesVersion>$(MainVersion)$(PackageVersionSuffix)</CaravelaSamplesVersion>
        <CaravelaSamplesAssemblyVersion>$(MainVersion)</CaravelaSamplesAssemblyVersion>
    </PropertyGroup>

    
    <!-- Versions of dependencies -->
    <PropertyGroup>
        <!-- Currently, only local builds are supported, so this property will be overwritten. -->
        <CaravelaVersion>LOCAL</CaravelaVersion>
    </PropertyGroup>

    <!-- Import overrides for the local build -->
    <Import Project="../artifacts/private/CaravelaDocumentationVersion.props" Condition="Exists('../artifacts/private/CaravelaDocumentationVersion.props')" />
    <Import Project="Dependencies.props" Condition="Exists('Dependencies.props')"/>

    

    <PropertyGroup>
        <AssemblyVersion>$(CaravelaDocumentationAssemblyVersion)</AssemblyVersion>
        <Version>$(CaravelaDocumentationVersion)</Version>
    </PropertyGroup>

</Project>
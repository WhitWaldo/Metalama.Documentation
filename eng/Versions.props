
<Project>

    <!-- Set the default version -->
    <Import Project="MainVersion.props" />
    
    <PropertyGroup>
        <MetalamaDocumentationVersion>$(MainVersion)$(PackageVersionSuffix)</MetalamaDocumentationVersion>
        <MetalamaDocumentationAssemblyVersion>$(MainVersion)</MetalamaDocumentationAssemblyVersion>
    </PropertyGroup>

    <!-- Versions of dependencies -->
    <PropertyGroup>
        <PostSharpEngineeringVersion>1.0.101-preview</PostSharpEngineeringVersion>
        <MetalamaVersion>branch:master</MetalamaVersion>
        <MetalamaVersion Condition="$(VcsBranch.StartsWith('master'))">0.5.53-preview</MetalamaVersion>
        <MetalamaCompilerVersion>branch:master</MetalamaCompilerVersion>
        <MetalamaCompilerVersion Condition="$(VcsBranch.StartsWith('master'))">4.3.1010-preview</MetalamaCompilerVersion>
        <MetalamaMigrationVersion>branch:dev</MetalamaMigrationVersion>
        <MetalamaMigrationVersion Condition="$(VcsBranch.StartsWith('master'))">0.5.73-preview</MetalamaMigrationVersion>
        <MetalamaExtensionsVersion>branch:dev</MetalamaExtensionsVersion>
        <MetalamaExtensionsVersion Condition="$(VcsBranch.StartsWith('master'))">0.5.73-preview</MetalamaExtensionsVersion>
		<NuGetCommandLineVersion>6.2.2</NuGetCommandLineVersion>
    </PropertyGroup>

    <!-- Import overrides for the local build -->
    <Import Project="Versions.g.props" Condition="Exists('Versions.g.props')" />

    <!-- Other properties dependent on the versions set above. -->
    <PropertyGroup>
        <AssemblyVersion>$(MetalamaDocumentationAssemblyVersion)</AssemblyVersion>
        <Version>$(MetalamaDocumentationVersion)</Version>
    </PropertyGroup>

</Project>
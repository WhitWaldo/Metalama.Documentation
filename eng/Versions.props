<Project>

    <!-- Set the default version -->
    <Import Project="MainVersion.props" />
    
    <PropertyGroup>
        <MetalamaSamplesVersion>$(MainVersion)$(PackageVersionSuffix)</MetalamaSamplesVersion>
        <MetalamaSamplesAssemblyVersion>$(MainVersion)</MetalamaSamplesAssemblyVersion>
    </PropertyGroup>

    
    <!-- Versions of dependencies -->
    <PropertyGroup>
        <!-- Currently, only local builds are supported, so this property will be overwritten. -->
        <MetalamaVersion>LOCAL</MetalamaVersion>
    </PropertyGroup>

    <!-- Import overrides for the local build -->
    <Import Project="../artifacts/private/MetalamaDocumentationVersion.props" Condition="Exists('../artifacts/private/MetalamaDocumentationVersion.props')" />
    <Import Project="Dependencies.props" Condition="Exists('Dependencies.props')"/>

    

    <PropertyGroup>
        <AssemblyVersion>$(MetalamaDocumentationAssemblyVersion)</AssemblyVersion>
        <Version>$(MetalamaDocumentationVersion)</Version>
    </PropertyGroup>

</Project>